alias dev='code ~/code/$(ls ~/code/ | fzf)'
alias pa="anime_stream $1"
alias i='paru --noconfirm -S'
alias s="paru -Ssq"
alias zipdir='function _zipdir() { zip -r "$1.zip" "$1"; }; _zipdir'
alias cpdf='find ./ -name "*.pptx" -exec soffice --headless --convert-to pdf {} \;'
alias cdocx='find ./ -name "*.docx" -exec soffice --headless --convert-to pdf {} \;'
alias whoami="ssh -i $HOME/.key/sahil.pem sahilsuman@20.193.158.252"